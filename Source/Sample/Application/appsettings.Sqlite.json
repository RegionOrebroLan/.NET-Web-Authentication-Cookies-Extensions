{
	"AllowedHosts": "*",
	"Authentication": {
		"Cookies": {
			"TicketStore": {
				"CookieAuthenticationSchemes": [
					"Cookies"
				],
				"Type": "RegionOrebroLan.Web.Authentication.Cookies.DistributedCacheTicketStore, RegionOrebroLan.Web.Authentication.Cookies"
			}
		}
	},
	"Caching": {
		"DistributedCache": {
			"Options": {
				"ConnectionStringName": "Cache"
			},
			"Provider": "Sqlite"
		}
	},
	"ConnectionStrings": {
		"Cache": "Data Source=|DataDirectory|Cache.db",
		"Database": "Data Source=|DataDirectory|Database.db",
		"DataProtection": "Data Source=|DataDirectory|DataProtection.db"
	},
	"DataProtection": {
		"Options": {
			"ConnectionStringName": "DataProtection",
			"KeyProtection": {
				"Options": {
					"CertificateResolver": {
						"Options": {
							"Password": "password",
							"Path": "Data/Key-Protection-Certificate.pfx"
						},
						"Type": "RegionOrebroLan.Security.Cryptography.Configuration.FileResolverOptions, RegionOrebroLan"
					}
				},
				"Type": "RegionOrebroLan.DataProtection.Configuration.KeyProtection.CertificateOptions, RegionOrebroLan.DataProtection"
			}
		},
		"Type": "RegionOrebroLan.DataProtection.Configuration.SqliteOptions, RegionOrebroLan.DataProtection"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Warning"
		}
	}
}